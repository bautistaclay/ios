[Script]
{{{爱阅书香}}} = type=http-response, pattern=^https:\/\/api-access\.pangolin-sdk-toutiao\.com\/api\/ad\/union\/sdk\/get_ads.+, script-path=https://raw.githubusercontent.com/bautistaclay/ios/master/Scripts/iFreeTime.js, requires-body=true, max-size=0, script-update-interval=0, timeout=10

[MITM]
{{{主机名}}} = %APPEND% api-access.pangolin-sdk-toutiao.com