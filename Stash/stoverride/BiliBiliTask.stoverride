cron:
  script:
# 哔哩哔哩每日任务
    - name: "哔哩哔哩每日任务"
      cron: "0 0 12 * * *"
      timeout: 300

script-providers:
  "哔哩哔哩每日任务":
    url: https://raw.githubusercontent.com/ClydeTime/BiliBili/main/js/BiliBiliDailyBonus.js
    interval: 86400